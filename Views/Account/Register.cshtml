@model RegisterViewModel

<form method="post">
  <input hidden="true" asp-for="@Model.ReturnUrl">
  <div class="mb-3">
    <label asp-for="@Model.UserName" class="form-label">Username</label>
    <input type="text" asp-for="@Model.UserName" class="form-control" placeholder="username">
  </div>
  <div class="mb-3">
    <label asp-for="@Model.CreatePassword" class="form-label">Password</label>
    <input type="password" asp-for="@Model.CreatePassword" class="form-control">
  </div>
  <button type="submit" class="btn btn-primary">Register</button>
</form>
